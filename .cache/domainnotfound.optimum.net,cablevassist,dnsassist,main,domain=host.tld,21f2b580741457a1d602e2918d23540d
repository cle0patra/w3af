<script type="text/javascript">
//var footer_hostName = document.getElementById('footer_HostnameDiv').innerHTML;
// rollover background for main navigation
	function footer_chgHeaderBg(obj,evnt){
		if (evnt == "over"){
			obj.style.backgroundColor='#39a5eb';
			obj.style.cursor='hand';
		}else{
			obj.style.backgroundColor='';
		}
	}

function footer_resetFontStyles(){
	document.getElementById('footer_web').style.fontWeight="normal";
	document.getElementById('footer_YPages').style.fontWeight="normal";		
	document.getElementById('footer_Music').style.fontWeight="normal";		
	document.getElementById('footer_TV').style.fontWeight="normal";		
}

function footer_getSrchForm(objId){
	var frmObj = document.footer_infospaceSearch;
	var hinputs='';
	var sinputs;
	var qcatVal;
	if (objId=="footer_YPages"){		
		//frmObj.action='http://www.switchboard.com/swy.5051/dir/results.htm';
		//frmObj.method='get';
		//frmObj.srchBtn.value='Search Yellow Pages';
		qcatVal='yp';
		hinputs='<input type="hidden" name="qcat" value="" />';
//		sinputs='<input type="text" name="KW" value="Business Name" style="font-size:142%;width:135px;" onfocus="(this.value==\'Business Name\')?this.value=\'\':\'\';" onblur="(this.value==\'\')?this.value=\'Business Name\':\'\';" />&nbsp;in&nbsp;<input type="text" name="LO" value="City, State or Zip" style="font-size:142%;width:290px;" onfocus="(this.value==\'City, State or Zip\')?this.value=\'\':\'\';" onblur="(this.value==\'\')?this.value=\'City, State or Zip\':\'\';" />';		
		sinputs='<input type="text" name="KW" value="Find Local Services" style="font-size:142%;width:135px;" onfocus="(this.value==\'Find Local Services\')?this.value=\'\':\'\';" onblur="(this.value==\'\')?this.value=\'Find Local Services\':\'\';" />&nbsp;in&nbsp;<input type="text" name="LO" value="City, State or Zip" style="font-size:142%;width:140px;" onfocus="(this.value==\'City, State or Zip\')?this.value=\'\':\'\';" onblur="(this.value==\'\')?this.value=\'City, State or Zip\':\'\';" />';		
		document.getElementById('footer_poweredByGoogle1').style.visibility="visible";
		document.getElementById('footer_poweredByGoogle1').style.display="block";
		document.getElementById('footer_poweredByGoogle2').style.display="none";		
	}
	else if (objId=="footer_Music"){
		frmObj.action='http://optimum.rhapsody.com/-search';	
		frmObj.method='get';
		//frmObj.srchBtn.value='Search Music';
		sinputs='<select name="searchtype" style="font-size:140%;width:140px;height:23px;"><option value="RhapKeyword">Search by Keyword</option><option value="RhapArtist">Search by Artist</option><option value="RhapTrack">Search by Track</option><option value="RhapAlbum">Search by Album</option><option value="RhapComposer">Search by Composer</option><option value="video">Search by Videos</option></select>&nbsp;<input type="text" name="query" value="" style="font-size:142%;width:147px;" />';		
		//hinputs='<input type="hidden" name="size" value="100" /><input type="hidden" name="availability" value="Stream" />';
		document.getElementById('footer_poweredByGoogle1').style.display="none";		
		document.getElementById('footer_poweredByGoogle2').style.visibility="visible";				
		document.getElementById('footer_poweredByGoogle2').style.display="block";		
	}
	else if (objId=="footer_TV"){
		frmObj.action=hostName + '/EPGRedirect';	
		frmObj.method='get';
		//frmObj.srchBtn.value='Search TV Listings';
		sinputs='<select name="searchField" style="font-size:140%;width:140px;margin:0px;padding:0px;height:23px;"><option value="short_title">Search By Title</option><option value="episode_title">Search By Episode Title</option><option value="short_description">Search By Description</option><option value="cast_members">Search By Cast Member</option></select>&nbsp;<input type="text" name="query" value="" style="font-size:142%;width:147px;" />';
		document.getElementById('footer_poweredByGoogle1').style.display="none";		
		document.getElementById('footer_poweredByGoogle2').style.visibility="visible";		
		document.getElementById('footer_poweredByGoogle2').style.display="block";				
	}	
	else{
		if (objId=="footer_web"){
			qcatVal='web';
			document.getElementById('footer_poweredByGoogle1').style.visibility="visible";
			document.getElementById('footer_poweredByGoogle1').style.display="none";					
			document.getElementById('footer_poweredByGoogle2').style.display="none";
		}
		hinputs='<input type="hidden" name="qcat" value="" /><input type="hidden" name="pgtarg" value="wbsdogpile" />';
		sinputs='<input type="text" name="qkw" style="font-size:142%;width:290px;"/>';
	}	
		
	// reset font style for all links	
	footer_resetFontStyles();
	
	document.getElementById(objId).style.fontWeight="bold";
	
	document.getElementById('footer_srchInputs').innerHTML=sinputs;
	document.getElementById('footer_hiddenInputs').innerHTML=hinputs;

	
	if(frmObj.qcat){		
		frmObj.qcat.value=qcatVal;
		if(frmObj.qkw!=null) frmObj.qkw.focus();
	}
}

// submit search form
function footer_submitSrch(){
	var frmObj = document.footer_infospaceSearch;
	if(frmObj.qcat!=null && frmObj.qcat.value=='web')
	{
//	    document.getElementsByName('idxSearch').item(0).qkw.value= document.infospaceSearch.qkw.value;
//        document.getElementsByName('idxSearch').item(0).submit(); 
       if(frmObj.qkw.value != ''){
		location.href = 'http://search.domainnotfound.optimum.net/cablevassistsearch/ws/results/dns/'+encodeURIComponent(frmObj.qkw.value)+'/1/4001/bottom/Relevance/iq=true/_iceUrlFlag=7?_IceUrl=true';
	    }	
		return false;
	}
	else if(frmObj.qcat!=null && frmObj.qcat.value=='yp')
	{
		location.href = 'http://www.optimum.net/Search/Local?q=' + encodeURIComponent(frmObj.KW.value) + '&l=' + encodeURIComponent(frmObj.LO.value);
		return false;
	}
	else
	{
		frmObj.submit();
	}
}


