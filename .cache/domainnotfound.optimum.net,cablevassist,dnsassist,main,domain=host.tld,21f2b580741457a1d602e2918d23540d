        function openModal(url) 
        {   
            var w = screen.availWidth; 
            var h = screen.availHeight; 
            var popW = w*0.6;
            var popH = h*0.6; 
            var leftPos = (w-popW)/2
            var topPos = (h-popH)/2; 
            if(window.showModalDialog != null && typeof(window.showModalDialog) != 'undefined')
                window.showModalDialog(url,null,'movable=0,resizable=no,status=no,toolbar=no, width=' + popW + ',height=' + popH + ',scrollbars=yes, location=no,top=' + topPos + ',left=' + leftPos+', menubar=no'); 
            else
                window.open(url,null,'movable=0,resizable=no,status=no,toolbar=no, width=' + popW + ',height=' + popH + ',scrollbars=yes, location=no,top=' + topPos + ',left=' + leftPos+', menubar=no'); 
        } 
    </script>    
